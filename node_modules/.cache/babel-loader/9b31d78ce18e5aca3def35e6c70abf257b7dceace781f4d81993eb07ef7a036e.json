11111111111111111111111111\", \"11111111111111111111111111111111\", \"11111111111111111111111111111111\", \"11111111111111111111111111111111\", \"11111111111111111111111111111111\", \"11111111111111111111111111111111\", \"11111111111111111111111111111111\", \"11111111111111111111111111111111\", \"11111111111111111111111111111111\", \"11111111111111111111111111111111\", \"11111111111111111111111111111111\", \"11111111111111111111111111111111\", \"11111111111111111111111111111111\", \"11111111111111111111111111111111\", \"11111111111111111111111111111111\", \"11111111111111111111111111111111\", \"11111111111111111111111111111111\", \"11111111111111111111111111111111\", \"11111111111111111111111111111111\", \"11111111111111111111111111111111\", \"11111111111111111111111111111111\", \"11111111111111111111111111111111\", \"11111111111111111111111111111111\"]\n      },\n      // Accessories\n      accessories: {\n        helmet: \"#FFD700\",\n        visor: \"#FF69B4\",\n        pilotBadge: \"#9370DB\",\n        flightSuit: \"#2C2C2C\"\n      }\n    },\n    // Enhanced animations\n    animations: {\n      idle: [{\n        x: 0,\n        y: 0,\n        scale: 1.0,\n        rotation: 0\n      }, {\n        x: 0,\n        y: -1,\n        scale: 1.0,\n        rotation: 0\n      }, {\n        x: 0,\n        y: 0,\n        scale: 1.0,\n        rotation: 0\n      }, {\n        x: 0,\n        y: -2,\n        scale: 1.0,\n        rotation: 0\n      }],\n      focused: [{\n        x: 0,\n        y: 0,\n        scale: 1.0,\n        rotation: 0\n      }, {\n        x: 0,\n        y: 0,\n        scale: 1.02,\n        rotation: 1\n      }, {\n        x: 0,\n        y: 0,\n        scale: 1.0,\n        rotation: 0\n      }, {\n        x: 0,\n        y: 0,\n        scale: 1.02,\n        rotation: -1\n      }],\n      confident: [{\n        x: 0,\n        y: -3,\n        scale: 1.05\n      }, {\n        x: 0,\n        y: -1,\n        scale: 1.0\n      }, {\n        x: 0,\n        y: -3,\n        scale: 1.05\n      }, {\n        x: 0,\n        y: -1,\n        scale: 1.0\n      }],\n      piloting: [{\n        x: 0,\n        y: 0,\n        scale: 1.0,\n        rotation: 0\n      }, {\n        x: -2,\n        y: 0,\n        scale: 1.0,\n        rotation: 0\n      }, {\n        x: 0,\n        y: 0,\n        scale: 1.0,\n        rotation: 0\n      }, {\n        x: 2,\n        y: 0,\n        scale: 1.0,\n        rotation: 0\n      }]\n    }\n  }\n};\n\n// Enhanced Sprite Renderer with better quality\nexport class EnhancedSpriteRenderer {\n  constructor() {\n    this.pixelSize = 2; // Scale up for better visibility\n  }\n\n  // Draw enhanced character sprite\n  drawEnhancedCharacter(ctx, character, x, y, animation = 'idle', frame = 0) {\n    const characterData = EnhancedCharacterDesigns[character];\n    if (!characterData) return;\n    const animFrame = characterData.animations[animation][frame % characterData.animations[animation].length];\n    const {\n      x: offsetX,\n      y: offsetY,\n      scale,\n      rotation = 0\n    } = animFrame;\n    ctx.save();\n    ctx.translate(x + offsetX, y + offsetY);\n    ctx.scale(scale, scale);\n    ctx.rotate(rotation * Math.PI / 180);\n\n    // Draw head\n    this.drawEnhancedSprite(ctx, characterData.sprite.head, 0, 0);\n\n    // Draw body\n    this.drawEnhancedSprite(ctx, characterData.sprite.body, 0, 32);\n\n    // Draw accessories\n    this.drawEnhancedAccessories(ctx, characterData.sprite.accessories, 0, 0);\n    ctx.restore();\n  }\n\n  // Draw enhanced sprite from pixel data\n  drawEnhancedSprite(ctx, spriteData, x, y) {\n    const {\n      pixels,\n      skinTone,\n      hairColor,\n      eyeColor,\n      mouthColor,\n      shirtColor,\n      pantsColor\n    } = spriteData;\n    for (let row = 0; row < pixels.length; row++) {\n      for (let col = 0; col < pixels[row].length; col++) {\n        if (pixels[row][col] === '1') {\n          // Determine color based on position and character\n          let color = skinTone; // Default skin tone\n\n          // Add more sophisticated color mapping based on research\n          if (row < 8) color = hairColor; // Hair area\n          else if (row >= 8 && row < 24) color = skinTone; // Face area\n          else if (row >= 24 && row < 32) color = shirtColor; // Shirt area\n          else if (row >= 32) color = pantsColor; // Pants area\n\n          ctx.fillStyle = color;\n          ctx.fillRect(x + col * this.pixelSize, y + row * this.pixelSize, this.pixelSize, this.pixelSize);\n        }\n      }\n    }\n  }\n\n  // Draw enhanced accessories\n  drawEnhancedAccessories(ctx, accessories, x, y) {\n    const {\n      helmet,\n      visor,\n      pilotBadge,\n      flightSuit\n    } = accessories;\n\n    // Draw helmet\n    ctx.fillStyle = helmet;\n    ctx.globalAlpha = 0.8;\n    ctx.beginPath();\n    ctx.arc(x + 16, y + 16, 14, 0, Math.PI * 2);\n    ctx.fill();\n\n    // Draw visor\n    ctx.fillStyle = visor;\n    ctx.globalAlpha = 0.6;\n    ctx.beginPath();\n    ctx.arc(x + 16, y + 16, 12, 0, Math.PI * 2);\n    ctx.fill();\n\n    // Draw pilot badge\n    ctx.fillStyle = pilotBadge;\n    ctx.globalAlpha = 1.0;\n    ctx.fillRect(x + 12, y + 20, 8, 8);\n    ctx.globalAlpha = 1.0;\n  }\n}\nexport default EnhancedCharacterDesigns;","map":{"version":3,"names":["EnhancedCharacterDesigns","kaden","name","age","personality","description","ship","sprite","head","skinTone","hairColor","eyeColor","mouthColor","pixels","body","shirtColor","pantsColor","accessories","helmet","visor","pilotBadge","flightSuit","animations","idle","x","y","scale","rotation","excited","determined","piloting","adelynn","focused","confident","EnhancedSpriteRenderer","constructor","pixelSize","drawEnhancedCharacter","ctx","character","animation","frame","characterData","animFrame","length","offsetX","offsetY","save","translate","rotate","Math","PI","drawEnhancedSprite","drawEnhancedAccessories","restore","spriteData","row","col","color","fillStyle","fillRect","global